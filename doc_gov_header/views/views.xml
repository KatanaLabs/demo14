<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="l10n_do_res_company_text_header_form" model="ir.ui.view">
        <field name="name">res_company text_header form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Government Config" name="government_config">
                    <group>
                        <group>
                            <field name="l10n_do_gov_text_header_document"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>